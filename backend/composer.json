{
    "name": "anco/api-backend",
    "description": "Backend REST API basado en CodeIgniter 3 con RESTServer y PostgreSQL",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "chriskacerguis/codeigniter-restserver": "^3.1",
        "firebase/php-jwt": "^6.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "application/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
